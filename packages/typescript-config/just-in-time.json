{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base.json",
  "compilerOptions": {
    "composite": false,
    "incremental": true,
    "declaration": true,
    "emitDeclarationOnly": false,
    "noEmit": true,
    "forceConsistentCasingInFileNames": true,
    "resolvePackageJsonExports": true
  }
}
