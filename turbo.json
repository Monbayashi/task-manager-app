{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "globalEnv": ["NODE_ENV"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**"],
      "env": []
    },
    "dev:prepare": {},
    "dev": {
      "dependsOn": ["^dev:prepare"],
      "cache": false,
      "persistent": true,
      "env": []
    },
    "localstack:dev": {
      "cache": false,
      "persistent": true
    },
    "test": {},
    "lint": {},
    "format": {}
  }
}
