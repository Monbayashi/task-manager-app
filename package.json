{
  "name": "task-manager-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prepare": "husky",
    "build": "turbo run build",
    "dev:prepare": "turbo run dev:prepare",
    "dev": "turbo run dev --parallel",
    "test": "turbo run test --continue",
    "lint": "turbo run lint",
    "format": "turbo run format",
    "localstack:up": "docker-compose up -d localstack && cd infra && pnpm deploy:pre-lambda && pnpm localstack:bootstrap && pnpm localstack:deploy",
    "localstack:down": "docker-compose down -v",
    "localstack:dev": "turbo run localstack:dev",
    "prune:backend": "turbo prune backend --docker"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "cross-env": "^10.1.0",
    "dotenv-cli": "^11.0.0",
    "husky": "^9.1.7",
    "nyc": "^17.1.0",
    "prettier": "^3.6.2",
    "turbo": "^2.6.1",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@10.21.0",
  "engines": {
    "node": ">=22",
    "pnpm": ">=10"
  }
}
